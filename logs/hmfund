<VirtualHost *:80>
	ServerAdmin admin@suretoo.com
        ServerName aws.suretoo.com
	DocumentRoot /home/ubuntu/project/cpro
        #ServerAlias aws.suretoo.com
        WSGIScriptAlias / /home/ubuntu/project/cpro/cpro/wsgi.py
	ErrorLog /home/ubuntu/project/cpro/logs/apache_error.log
	CustomLog /home/ubuntu/project/cpro/logs/apache_access.log combined
	<Directory /home/ubuntu/project/cpro/media>
		Order deny,allow
		Allow from all
	</Directory>
        <Directory /home/ubuntu/project/cpro/media/uploads>
                Order deny,allow
                Allow from all
        </Directory>
        <Directory /home/ubuntu/project/cpro/media/uploads/Image>
                Order deny,allow
                Allow from all
        </Directory>

	<Directory /home/ubuntu/project/cpro/static>
                Order deny,allow
                Allow from all
        </Directory>

	LogLevel warn
	#WSGIDaemonProcess 54.255.159.102 user=ubuntu processes=2 threads=25
	#WSGIProcessGroup 54.255.159.102
	alias /media/ /home/ubuntu/project/cpro/media/
	alias /static/ /home/ubuntu/project/cpro/static/
</VirtualHost>
